{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "link[sizes]",
          "meta[name=theme-color]"
        ]
      }
    ],
    "disown-opener": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "flex-grow",
          "user-select"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ie <= 11"
  ]
}